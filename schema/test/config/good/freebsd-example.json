{
    "ociVersion": "1.3.0",
    "process": {
        "terminal": true,
        "args": [
            "sh"
        ],
        "env": [
            "PATH=/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin",
            "TERM=xterm"
        ],
        "cwd": "/"
    },
    "root": {
        "path": "rootfs"
    },
    "hostname": "slartibartfast",
    "mounts": [
        {
            "destination": "/dev",
            "type": "devfs",
            "source": "devfs",
            "options": [
                "ruleset=4"
            ]
        },
        {
            "destination": "/dev/fd",
            "type": "fdescfs",
            "source": "fdescfs",
            "options": []
        }
    ],
    "freebsd": {
        "devices": [
            {
                "path": "pf",
                "mode": 448
            }
        ],
        "jail": {
            "host": "new",
            "vnet": "new",
            "enforceStatfs": 1,
            "allow": {
                "rawSockets": true,
                "chflags": true,
                "mount": [
                    "tmpfs"
                ]
            }
        }
    }
}
